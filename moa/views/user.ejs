<!DOCTYPE html>
<html ng-app="moa">
<head>
    <title>MOA</title>
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, user-scalable=0, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="apple-mobile-web-app-status-bar-style" content="black"/>
    <meta name="apple-mobile-web-app-capable" content="yes"/>

    <link rel='stylesheet' href='/stylesheets/style.css'/>

    <link rel="stylesheet" href="/bootstrap-3.2.0/css/bootstrap.min.css">
    <script src="/javascripts/jquery-1.11.1.js" type="text/javascript"></script>
    <script src="/bootstrap-3.2.0/js/bootstrap.min.js" type="text/javascript"></script>

    <script src="/javascripts/angular-1.2.9/angular.js" type="text/javascript"></script>
    <script src="/javascripts/angular-1.2.9/angular-route.js" type="text/javascript"></script>

    <script src="/moa.js" type="text/javascript"></script>
</head>
<body ng-controller="main" class="container">

<div class="col" style="margin-top:20px;">

    <div class="col-sm-6">
        <div class="form-group">
            <label for="code" class="col-sm-4 control-label">编码</label>
            <div class="col-sm-8">
                <input type="text" readonly class="form-control" id="code" name="code" value="<%= code %>">
            </div>
        </div>
    </div>

    <div class="col-sm-6">
        <div class="form-group">
            <label for="name" class="col-sm-4 control-label">名字</label>
            <div class="col-sm-8">
                <input type="text" readonly class="form-control" id="name" name="name" value="<%= name %>">
            </div>
        </div>
    </div>

</div>

</body>

<script type="text/javascript">

    angular.module('moa', ['moaDirective'])
            .controller('main', function ($scope) {

            });

</script>

</html>
